{
	"name": "netherphp/blog",
	"description": "Blog plugin for Nether Atlantis",
	"keywords": [ "netherphp", "blog" ],
	"license": "BSD-2-Clause",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors":[
		{
			"name": "Bob Majdak Jr",
			"email": "bmajdak@php.net",
			"homepage": "http://pegasusgate.net/+bob",
			"role": "developer"
		}
	],
	"config": {
		"preferred-install": {
			"netherphp/*": "source",
			"*": "dist"
		},
		"allow-plugins": {
			"netherphp/composer-plugin": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require": {
		"php": "^8.1",
		"netherphp/atlantis": "dev-master"
	},
	"require-dev": {
		"netherphp/standards": ">=0.2.9",
		"phpunit/phpunit": "^9.6",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
	},
	"bin": [
		"bin/nblog"
	],
	"autoload": {
		"psr-0": {
			"Nether\\":"src/"
		}
	}
}
